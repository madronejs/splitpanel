{
  "name": "@madronejs/splitpanel",
  "version": "0.0.19",
  "private": false,
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/*.js",
    "dist/*.mjs",
    "dist/*.css",
    "types/**/*.d.ts"
  ],
  "keywords": [
    "splitpanel",
    "splitpane",
    "panel",
    "drag",
    "draggable",
    "resizable",
    "madrone",
    "vue",
    "web components"
  ],
  "types": "types/core/index.d.ts",
  "typesVersions": {
    "*": {
      "vue3": [
        "./types/render/vue3.d.ts"
      ],
      "webc": [
        "./types/render/webComponent.d.ts"
      ],
      "animate": [
        "./types/plugins/animate.d.ts"
      ],
      "draggable": [
        "./types/plugins/draggable.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./vue3": {
      "import": "./dist/vue3.js",
      "require": "./dist/vue3.cjs"
    },
    "./webc": {
      "import": "./dist/webComponent.js",
      "require": "./dist/webComponent.cjs"
    },
    "./animate": {
      "import": "./dist/animate.js",
      "require": "./dist/animate.cjs"
    },
    "./draggable": {
      "import": "./dist/draggable.js",
      "require": "./dist/draggable.cjs"
    },
    "./style": "./dist/splitpanel.css"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "build-dev": "NODE_ENV=development pnpm build-all",
    "build-types": "rm -rf types/* && vue-tsc -p ./tsconfig.json && tsc-alias -p tsconfig.json",
    "build": "rm -rf dist/* && vite build",
    "build-all": "pnpm build-types && pnpm build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@eslint/compat": "~1.2.9",
    "@eslint/eslintrc": "~3.3.1",
    "@eslint/js": "~9.26.0",
    "@stylistic/eslint-plugin": "~4.2.0",
    "@types/animejs": "^3.1.13",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "eslint": "~9.26.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "~16.1.0",
    "sass": "^1.88.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.8.3",
    "typescript-eslint": "8.32.1",
    "vite": "^6.3.5",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.10"
  },
  "dependencies": {
    "drag-drop-touch": "^1.3.1",
    "lodash": "^4.17.21",
    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "@madronejs/core": "^1.0.16",
    "animejs": "^3.2.2",
    "vue": "^3.5.12"
  },
  "engines": {
    "node": "~20.11 || ~22.14",
    "pnpm": ">= 10"
  },
  "packageManager": "pnpm@10.11.0"
}
